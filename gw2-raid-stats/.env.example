# Database
DB_PASSWORD=your_secure_password_here

# Admin Panel Password
ADMIN_PASSWORD=your_admin_password_here

# Cloudflare Tunnel (for production deployment)
# Get this from: cloudflared tunnel token <tunnel-name>
TUNNEL_TOKEN=your_cloudflare_tunnel_token

# Guild Configuration (can also be set in appsettings.json)
GUILD_NAME=My Guild
GUILD_TAG=TAG

# Number of parallel GW2EI processing workers
PROCESSOR_WORKERS=4

# Path to data directory (for TrueNAS mount, use /mnt/gw2-logs)
DATA_PATH=./data
